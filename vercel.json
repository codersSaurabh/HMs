{
    "version": 2,
    "builds": [
      {
        "src": "HMs/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "55mb", "runtime": "python3.12" }
      },
     
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "HMs/wsgi.py"
      }
    ]
  }
  
  
